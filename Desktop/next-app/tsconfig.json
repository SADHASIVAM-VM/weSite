{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/about/[...slug]/page.jsx", "app/about/page.jsx", "app/page.jsx", "app/layout.jsx", "components/AppLicationSta/ApplicationDisplay.jsx", "components/AppLicationSta/ApplicationItem.jsx", "components/Webdev.jsx", "components/Team.jsx", "components/ProjectsDt.jsx", "components/Navbar.jsx", "components/Footer.jsx", "components/Discuss.jsx", "components/FAQ.jsx", "components/Projects/ProudSec.jsx", "components/Projects/ProjectsDisplay.jsx"],
  "exclude": ["node_modules"]
}
